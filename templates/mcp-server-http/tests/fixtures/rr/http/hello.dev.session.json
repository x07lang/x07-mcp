{
  "schema_version": "x07.mcp.rr.http_session@0.1.0",
  "id": "hello.dev",
  "profile": "dev",
  "project_files": {
    "server": "config/mcp.server.json",
    "tools": "config/mcp.tools.json",
    "oauth": "config/mcp.oauth.json"
  },
  "base_url": "http://127.0.0.1:8314",
  "mcp_path": "/mcp",
  "steps": [
    {
      "name": "initialize",
      "req": {
        "method": "POST",
        "path": "/mcp",
        "headers": { "MCP-Protocol-Version": "2025-11-25" },
        "body_json": { "jsonrpc": "2.0", "id": 1, "method": "initialize" }
      },
      "res": {
        "status": 200,
        "headers": { "MCP-Session-Id": "sess_test_0001" },
        "body_json": { "jsonrpc": "2.0", "id": 1, "result": {} }
      }
    },
    {
      "name": "tools_list",
      "req": {
        "method": "POST",
        "path": "/mcp",
        "headers": {
          "MCP-Protocol-Version": "2025-11-25",
          "MCP-Session-Id": "sess_test_0001"
        },
        "body_json": { "jsonrpc": "2.0", "id": 2, "method": "tools/list" }
      },
      "res": {
        "status": 200,
        "headers": {},
        "body_json": {
          "jsonrpc": "2.0",
          "id": 2,
          "result": {
            "tools": [
              { "name": "hello.greet", "annotations": { "readOnlyHint": true } }
            ]
          }
        }
      }
    },
    {
      "name": "tools_call",
      "req": {
        "method": "POST",
        "path": "/mcp",
        "headers": {
          "MCP-Protocol-Version": "2025-11-25",
          "MCP-Session-Id": "sess_test_0001"
        },
        "body_json": {
          "jsonrpc": "2.0",
          "id": 3,
          "method": "tools/call",
          "params": { "name": "hello.greet" }
        }
      },
      "res": {
        "status": 200,
        "headers": {},
        "body_json": {
          "jsonrpc": "2.0",
          "id": 3,
          "result": { "content": [{ "type": "text", "text": "hello_x07_mcp_http" }] }
        }
      }
    }
  ]
}
